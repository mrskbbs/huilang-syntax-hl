{
  "name": "hui-lang",
  "displayName": "HuiLang",
  "description": "Syntax highlighting for your precious HuiLang!",
  "version": "6.9.420",
  "publisher": "mrskbbs",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "huilang",
      "aliases": ["HuiLang", "huilang"],
      "extensions": [".hui",".huilang",".hl"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "huilang",
      "scopeName": "source.hui",
      "path": "./syntaxes/huilang.tmLanguage.json"
    }]
  }
}
